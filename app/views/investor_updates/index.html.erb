<p id="notice"><%= notice %></p>

<h1>Listing Investor Updates</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Summary</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @investor_updates.each do |investor_update| %>
      <tr>
        <td><%= investor_update.title %></td>
        <td><%= investor_update.summary %></td>
        <td><%= link_to 'Show', investor_update %></td>
        <td><%= link_to 'Edit', edit_investor_update_path(investor_update) %></td>
        <td><%= link_to 'Destroy', investor_update, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Investor update', new_investor_update_path %>
